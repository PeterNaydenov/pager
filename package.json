{
  "name": "@peter.naydenov/pager",
  "description": "A library to split results into multiple pages",
  "version": "1.0.3",
  "license": "MIT",
  "author": "Peter Naydenov",
  "main": "src/main.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/main.js",
      "require": "./dist/pager.cjs",
      "default": "./src/main.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./src/*": "./src/*"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "mocha test",
    "cover": "c8 mocha test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeterNaydenov/pager.git"
  },
  "keywords": [
    "result",
    "pager"
  ],
  "dependencies": {
    "@peter.naydenov/stack": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "c8": "^10.1.3",
    "chai": "^5.2.0",
    "mocha": "^11.1.0",
    "rollup": "^4.35.0"
  },
  "c8": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "node_modules",
      "test",
      "test-data"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
